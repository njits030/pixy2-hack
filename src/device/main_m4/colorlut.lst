N#define SPARE_CAPACITY   16
N
Ntemplate <typename Object> class SimpleVector
N{
Npublic:
N
N    SimpleVector(int initSize = 0)
N        : m_size(0), m_capacity(initSize + SPARE_CAPACITY)
X        : m_size(0), m_capacity(initSize + 16)
N    { m_objects = new Object[m_capacity]; }
N
N    ~SimpleVector()
N    { delete [] m_objects; }
N
N    int resize(int newCapacity)
N    {
N        if(newCapacity < m_size)
N            return 0;
N
N        Object *oldArray = m_objects;
N
N        m_objects = new (std::nothrow) Object[newCapacity];
N        if (m_objects==NULL)
X        if (m_objects==0)
N        {
N            m_objects = oldArray;
N            return -1;
N        }
N        for(int k = 0; k<m_size; k++)
N            m_objects[k] = oldArray[k];
N
N        m_capacity = newCapacity;
N
N        delete [] oldArray;
N        return 0;
N    }
N
N    Object & operator[](int index)
N    { return m_objects[index]; }
N
N    const Object& operator[](int index) const
N    { return m_objects[index]; }
N
N    bool empty() const
N    { return size()==0; }
N
N    int size() const
N    { return m_size; }
N
N    int capacity() const
N    { return m_capacity; }
N
N	const Object *data()
N	{ return m_objects; }
N
N    int push_back(const Object& x)
N    {
N        if(m_size == m_capacity)
N            if (resize(m_capacity + SPARE_CAPACITY)<0)
X            if (resize(m_capacity + 16)<0)
N                return -1;
N        m_objects[m_size++] = x;
N        return 0;
N    }
N
N    void pop_back()
N    { m_size--; }
N
N	void clear()
N	{ m_size = 0; }
N
Nprivate:
N    int m_size;
N    int m_capacity;
N    Object *m_objects;
N};
N
