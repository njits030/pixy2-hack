Nbool IterPixel::reset(bool cleari)
N{
N    if (cleari)
N        m_i = 0;
N    if (m_points)
N    {
N        if (m_points->size()>m_i)
N        {
N            m_region = RectA((*m_points)[m_i].m_x, (*m_points)[m_i].m_y, CL_GROW_INC, CL_GROW_INC);
X            m_region = RectA((*m_points)[m_i].m_x, (*m_points)[m_i].m_y, 4, 4);
N            m_i++;
N        }
N        else
N            return false; // empty!
N    }
N    m_x = m_y = 0;
N    m_pixels = m_frame.m_pixels + (m_region.m_yOffset | 1)*m_frame.m_width + (m_region.m_xOffset | 1);
N    return true;
N}
