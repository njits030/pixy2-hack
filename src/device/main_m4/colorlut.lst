N
Nbool ColorLUT::growRegion(RectA *region, const Frame8 &frame, uint8_t dir)
N{
N    if (dir==0) // grow left
N    {
N        if (region->m_xOffset>=CL_GROW_INC)
X        if (region->m_xOffset>=4)
N        {
N            region->m_xOffset -= CL_GROW_INC;
X            region->m_xOffset -= 4;
N            region->m_width += CL_GROW_INC;
X            region->m_width += 4;
N        }
N        else
N            return true;
N    }
N    else if (dir==1) // grow top
N    {
N        if (region->m_yOffset>=CL_GROW_INC)
X        if (region->m_yOffset>=4)
N        {
N            region->m_yOffset -= CL_GROW_INC;
X            region->m_yOffset -= 4;
N            region->m_height += CL_GROW_INC;
X            region->m_height += 4;
N        }
N        else
N            return true;
N    }
N    else if (dir==2) // grow right
N    {
N        if (region->m_xOffset+region->m_width+CL_GROW_INC>frame.m_width)
X        if (region->m_xOffset+region->m_width+4>frame.m_width)
N            return true;
N        region->m_width += CL_GROW_INC;
X        region->m_width += 4;
N    }
N    else if (dir==3) // grow bottom
N    {
N        if (region->m_yOffset+region->m_height+CL_GROW_INC>frame.m_height)
X        if (region->m_yOffset+region->m_height+4>frame.m_height)
N            return true;
N        region->m_height += CL_GROW_INC;
X        region->m_height += 4;
N    }
N    return false;
N}
N
N
Nfloat ColorLUT::testRegion(const RectA &region, const Frame8 &frame, UVPixel *mean, Points *points)
N{
N    UVPixel subMean;
N    float distance;
N    RectA subRegion(0, 0, CL_GROW_INC, CL_GROW_INC);
X    RectA subRegion(0, 0, 4, 4);
N    subRegion.m_xOffset = region.m_xOffset;
N    subRegion.m_yOffset = region.m_yOffset;
N    bool horiz = region.m_width>region.m_height;
N    uint32_t i, test, endpoint = horiz ? region.m_width : region.m_height;
N
N    for (i=0, test=0; i<endpoint; i+=CL_GROW_INC)
X    for (i=0, test=0; i<endpoint; i+=4)
N    {
N        getMean(subRegion, frame, &subMean);
N        distance = sqrt((float)((mean->m_u-subMean.m_u)*(mean->m_u-subMean.m_u) + (mean->m_v-subMean.m_v)*(mean->m_v-subMean.m_v)));
N        if ((uint32_t)distance<m_maxDist)
N        {
N            int32_t n = points->size();
N            mean->m_u = ((longlong)mean->m_u*n + subMean.m_u)/(n+1);
N            mean->m_v = ((longlong)mean->m_v*n + subMean.m_v)/(n+1);
N            if (points->push_back(Point16(subRegion.m_xOffset, subRegion.m_yOffset))<0)
N                break;
N            //DBG("add %d %d %d", subRegion.m_xOffset, subRegion.m_yOffset, points->size());
N            test++;
N        }
N
N        if (horiz)
N            subRegion.m_xOffset += CL_GROW_INC;
X            subRegion.m_xOffset += 4;
N        else
N            subRegion.m_yOffset += CL_GROW_INC;
X            subRegion.m_yOffset += 4;
N    }
N
N    //DBG("return %f", (float)test*CL_GROW_INC/endpoint);
N    return (float)test*CL_GROW_INC/endpoint;
X    return (float)test*4/endpoint;
N}
N
N
Nvoid ColorLUT::growRegion(const Frame8 &frame, const Point16 &seed, Points *points)
N{
N    uint8_t dir, done;
N    RectA region, newRegion;
N    UVPixel mean;
N    float ratio;
N
N    done = 0;
N
N    // create seed 2*CL_GROW_INCx2*CL_GROW_INC region from seed position, make sure it's within the frame
N    region.m_xOffset = seed.m_x;
N    region.m_yOffset = seed.m_y;
N    if (growRegion(&region, frame, 0))
N        done |= 1<<0;
N    else
N        points->push_back(Point16(region.m_xOffset, region.m_yOffset));
N    if (growRegion(&region, frame, 1))
N        done |= 1<<1;
N    else
N        points->push_back(Point16(region.m_xOffset, region.m_yOffset));
N    if (growRegion(&region, frame, 2))
N        done |= 1<<2;
N    else
N        points->push_back(Point16(seed.m_x, region.m_yOffset));
N    if (growRegion(&region, frame, 3))
N        done |= 1<<3;
N    else
N        points->push_back(seed);
N
N    getMean(region, frame, &mean);
N
N    while(done!=0x0f)
N    {
N        for (dir=0; dir<4; dir++)
N        {
N            newRegion = region;
N            if (done&(1<<dir))
N                continue;
N            else if (dir==0) // left
N                newRegion.m_width = 0;
N            else if (dir==1) // top
N                newRegion.m_height = 0; // top and bottom
N            else if (dir==2) // right
N            {
N                newRegion.m_xOffset += newRegion.m_width;
N                newRegion.m_width = 0;
N            }
N            else if (dir==3) // bottom
N            {
N                newRegion.m_yOffset += newRegion.m_height;
N                newRegion.m_height = 0;
N            }
N
N            if (growRegion(&newRegion, frame, dir))
N                done |= 1<<dir;
N            else
N            {
N                ratio = testRegion(newRegion, frame, &mean, points);
N                if (ratio<m_minRatio)
N                    done |= 1<<dir;
N                else
N                    growRegion(&region, frame, dir);
N            }
N        }
N    }
N}
N
N
Nvoid ColorLUT::getMean(const RectA &region ,const Frame8 &frame, UVPixel *mean)
N{
N    UVPixel uv;
N    uint32_t n=0;
N    IterPixel ip(frame, region);
N
N    longlong usum=0, vsum=0;
N
N    while(ip.next(&uv))
N    {
N        usum += uv.m_u;
N        vsum += uv.m_v;
N        n++;
N    }
N
N    mean->m_u = usum/n;
N    mean->m_v = vsum/n;
N}
