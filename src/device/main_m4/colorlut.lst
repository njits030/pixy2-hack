Nuint32_t IterPixel::averageRgb(uint32_t *pixels)
N{
N	RGBPixel rgb;
N	uint32_t r, g, b, n;
N	reset();
N	for (r=g=b=n=0; next(NULL, &rgb); n++)
X	for (r=g=b=n=0; next(0, &rgb); n++)
N	{
N		r += rgb.m_r;
N		g += rgb.m_g;
N		b += rgb.m_b;		
N	}
N
N	r /= n;
N	g /= n;
N	b /= n;
N
N	if (pixels)
N		*pixels = n;
N	return (r<<16) | (g<<8) | b;
N}
