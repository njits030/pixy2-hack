Nint ColorLUT::generateLUT()
N{
N    int32_t r, g, b, u, v, y, bin, sig;
N
N    clearLUT();
N
N    // recalc bounds for each signature
N    for (r=0; r<CL_NUM_SIGNATURES; r++)
X    for (r=0; r<7; r++)
N        updateSignature(r+1);
N
N    for (r=0; r<1<<8; r+=1<<(8-CL_LUT_COMPONENT_SCALE))
X    for (r=0; r<1<<8; r+=1<<(8-6))
N    {
N        for (g=0; g<1<<8; g+=1<<(8-CL_LUT_COMPONENT_SCALE))
X        for (g=0; g<1<<8; g+=1<<(8-6))
N        {
N            for (b=0; b<1<<8; b+=1<<(8-CL_LUT_COMPONENT_SCALE))
X            for (b=0; b<1<<8; b+=1<<(8-6))
N            {
N                y = r+g+b;
N
N                if (y<(int32_t)m_miny)
N                    continue;
N                u = ((r-g)<<CL_LUT_ENTRY_SCALE)/y;
X                u = ((r-g)<<15)/y;
N                v = ((b-g)<<CL_LUT_ENTRY_SCALE)/y;
X                v = ((b-g)<<15)/y;
N
N                for (sig=0; sig<CL_NUM_SIGNATURES; sig++)
X                for (sig=0; sig<7; sig++)
N                {
N                    if (m_signatures[sig].m_uMin==0 && m_signatures[sig].m_uMax==0)
N                        continue;
N                    if ((m_runtimeSigs[sig].m_uMin<u) && (u<m_runtimeSigs[sig].m_uMax) &&
N                            (m_runtimeSigs[sig].m_vMin<v) && (v<m_runtimeSigs[sig].m_vMax))
N                    {
N                        u = r-g;
N                        u >>= 9-CL_LUT_COMPONENT_SCALE;
X                        u >>= 9-6;
N                        u &= (1<<CL_LUT_COMPONENT_SCALE)-1;
X                        u &= (1<<6)-1;
N                        v = b-g;
N                        v >>= 9-CL_LUT_COMPONENT_SCALE;
X                        v >>= 9-6;
N                        v &= (1<<CL_LUT_COMPONENT_SCALE)-1;
X                        v &= (1<<6)-1;
N
N                        bin = (u<<CL_LUT_COMPONENT_SCALE)+ v;
X                        bin = (u<<6)+ v;
N
N                        if (m_lut[bin]==0 || m_lut[bin]>sig+1)
N                            m_lut[bin] = sig+1;
N                    }
N                }
N            }
N        }
N    }
N
N    return 0;
N}
N
N
Nvoid ColorLUT::clearLUT(uint8_t signum)
N{
N    int i;
N
N    for (i=0; i<CL_LUT_SIZE; i++)
X    for (i=0; i<(1<<(6*2)); i++)
N    {
N        if (signum==0)
N            m_lut[i] = 0;
N        else if (m_lut[i]==signum)
N            m_lut[i] = 0;
N    }
N}
