N
Nvoid ColorLUT::iterate(IterPixel *ip, ColorSignature *sig)
N{
N    int32_t scale;
N    float ratios[4];
N
N    // binary search -- this rouine is guaranteed to find the right value +/- 1, which is good enough!
N    // find all four values, umin, umax, vmin, vmax simultaneously
N    for (scale=1<<30, sig->m_uMin=sig->m_uMax=sig->m_vMin=sig->m_vMax=0; scale!=0; scale>>=1)
N    {
N        calcRatios(ip, sig, ratios);
N        if (ratios[0]>m_ratio)
N            sig->m_uMin += scale;
N        else
N            sig->m_uMin -= scale;
N
N        if (ratios[1]>m_ratio)
N            sig->m_uMax -= scale;
N        else
N            sig->m_uMax += scale;
N
N        if (ratios[2]>m_ratio)
N            sig->m_vMin += scale;
N        else
N            sig->m_vMin -= scale;
N
N        if (ratios[3]>m_ratio)
N            sig->m_vMax -= scale;
N        else
N            sig->m_vMax += scale;
N    }
N}
