Nbool IterPixel::next(UVPixel *uv, RGBPixel *rgb)
N{
N    if (m_points)
N    {
N        if (nextHelper(uv, rgb))
N            return true; // working on the current block
N        else // get new block
N        {
N            if (reset(false)) // reset indexes, increment m_i, get new block
N                return nextHelper(uv, rgb);  // we have another block!
N            else
N                return false; // blocks are empty
N        }
N    }
N    else
N        return nextHelper(uv, rgb);
N}
N
N
Nbool IterPixel::nextHelper(UVPixel *uv, RGBPixel *rgb)
N{
N    int32_t r, g1, g2, b, u, v, c, miny=CL_MIN_Y;
X    int32_t r, g1, g2, b, u, v, c, miny=(int32_t)(3*((1<<8)-1)*0.05);
N
N    while(1)
N    {
N        if (m_x>=m_region.m_width)
N        {
N            m_x = 0;
N            m_y += 2;
N            m_pixels += m_frame.m_width*2;
N        }
N        if (m_y>=m_region.m_height)
N            return false;
N
N        r = m_pixels[m_x];
N        g1 = m_pixels[m_x - 1];
N        g2 = m_pixels[-m_frame.m_width + m_x];
N        b = m_pixels[-m_frame.m_width + m_x - 1];
N		if (rgb)
N		{
N		  	rgb->m_r = r;
N			rgb->m_g = (g1+g2)/2;
N			rgb->m_b = b;
N		}
N		if (uv)
N		{
N        	c = r+g1+b;
N            if (c<miny)
N			{
N				m_x += 2;
N            	continue;
N			}
N        	u = ((r-g1)<<CL_LUT_ENTRY_SCALE)/c;
X        	u = ((r-g1)<<15)/c;
N        	c = r+g2+b;
N            if (c<miny)
N			{
N				m_x += 2;
N            	continue;
N			}
N        	v = ((b-g2)<<CL_LUT_ENTRY_SCALE)/c;
X        	v = ((b-g2)<<15)/c;
N
N        	uv->m_u = u;
N        	uv->m_v = v;
N		}
N
N		m_x += 2;
N        return true;
N    }
N}
